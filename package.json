{
  "name": "vizz",
  "version": "1.0.0",
  "description": "### A movie rental app",
  "main": "index.js",
  "scripts": {
    "test": "jest --testEnvironment=node --verbose --maxWorkers=1",
    "start": "npm run start:prod || npm run start:dev",
    "start:prod": "set NODE_ENV=default&& node index.js",
    "start:dev": "set NODE_ENV=test&& nodemon index.js",
    "client": "cd client && npm run start",
    "seed": "node scripts/seed.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": "14.15.1"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "config": "^1.29.4",
    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "express-async-errors": "^2.1.0",
    "fawn": "^2.1.5",
    "heroku": "^7.47.5",
    "joi-oid": "^1.1.2",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.29.1",
    "mongoose": "^5.0.2",
    "winston": "^2.4.0",
    "winston-mongodb": "^3.0.0"
  },
  "devDependencies": {
    "jest": "^22.2.2",
    "supertest": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JayDub21/Vizz.git"
  }
}
